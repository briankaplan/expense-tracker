{
  "name": "expense-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cleanup": "ts-node --transpile-only scripts/cleanup.ts",
    "move": "ts-node --transpile-only scripts/move-files.ts",
    "verify:structure": "ts-node --transpile-only scripts/verify-structure.ts",
    "verify:imports": "ts-node --transpile-only scripts/verify-imports.ts",
    "verify:cleanup": "ts-node --transpile-only scripts/verify-cleanup.ts",
    "check:integration": "ts-node --transpile-only scripts/check-integration.ts",
    "check:critical": "ts-node --transpile-only scripts/check-critical.ts",
    "compare:reference": "ts-node --transpile-only scripts/compare-with-reference.ts",
    "setup": "npm run cleanup && npm run copy-reference && npm run move && npm run verify:cleanup",
    "copy-reference": "ts-node --transpile-only scripts/copy-from-reference.ts",
    "check:all": "npm run check:integration && npm run compare:reference",
    "verify:all": "npm run check:all && npm run check:critical && npm run check:implementation",
    "next:steps": "ts-node --transpile-only scripts/show-next-steps.ts",
    "check:reference": "ts-node --transpile-only scripts/check-reference.ts",
    "verify:components": "ts-node --transpile-only scripts/verify-components.ts",
    "check:implementation": "ts-node --transpile-only scripts/check-implementation.ts"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@tanstack/react-table": "^8.20.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "lucide-react": "^0.469.0",
    "next": "15.1.3",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/glob": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chalk": "^4.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "glob": "^10.3.10",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "autoprefixer": "^10.4.16"
  }
}
